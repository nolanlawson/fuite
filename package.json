{
  "name": "fuite",
  "version": "5.0.7",
  "description": "A tool for finding memory leaks in web apps",
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "bin": "./src/cli.cjs",
  "exports": {
    ".": "./src/index.js"
  },
  "types": "./types/index.d.ts",
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "test": "run-p --race test:serve test:runner",
    "test:serve": "serve --no-request-logging",
    "test:runner": "node --test ./test/spec/*.test.js",
    "lint": "standard",
    "version": "run-s version:changelog version:add",
    "version:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version:add": "git add CHANGELOG.md"
  },
  "dependencies": {
    "as-table": "^1.0.55",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "exit-hook": "4.0.0",
    "heap-snapshot-toolkit": "^1.1.3",
    "ono": "^7.1.3",
    "ora": "^8.2.0",
    "please-upgrade-node": "^3.2.0",
    "pretty-bytes": "^6.1.1",
    "puppeteer": "^24.15.0",
    "quick-lru": "^7.0.1",
    "source-map": "^0.7.6",
    "stacktrace-parser": "^0.1.11",
    "table": "^6.9.0"
  },
  "devDependencies": {
    "chai": "^5.2.1",
    "conventional-changelog-cli": "^5.0.0",
    "navigo": "^8.11.1",
    "npm-run-all": "^4.1.5",
    "serve": "^14.2.4",
    "standard": "^17.1.2",
    "wait-for-localhost": "^4.1.0"
  },
  "keywords": [
    "memory",
    "leak",
    "detector"
  ],
  "author": "Nolan Lawson <nolan@nolanlawson.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nolanlawson/fuite.git"
  },
  "bugs": {
    "url": "https://github.com/nolanlawson/fuite/issues"
  },
  "homepage": "https://github.com/nolanlawson/fuite#readme",
  "engines": {
    "node": ">= 18.0.0"
  },
  "standard": {
    "ignore": [
      "thirdparty"
    ]
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
