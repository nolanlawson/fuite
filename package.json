{
  "name": "fuite",
  "version": "4.0.9",
  "description": "A tool for finding memory leaks in web apps",
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "bin": "./src/cli.cjs",
  "exports": {
    ".": "./src/index.js"
  },
  "types": "./types/index.d.ts",
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "test": "run-p --race test:serve test:runner",
