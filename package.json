{
  "name": "fuite",
  "version": "3.0.0",
  "description": "A tool for finding memory leaks in web apps",
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "bin": "./src/cli.cjs",
  "exports": {
    ".": "./src/index.js"
  },
  "types": "./types/index.d.ts",
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "test": "run-p --race test:serve test:mocha",
    "test:serve": "serve --no-request-logging",
    "test:mocha": "mocha ./test/spec/",
    "lint": "standard",
    "version": "run-s version:changelog version:add",
    "version:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version:add": "git add CHANGELOG.md"
  },
  "dependencies": {
    "as-table": "^1.0.55",
    "atob": "^2.1.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "crypto-random-string": "^5.0.0",
    "exit-hook": "4.0.0",
    "node-fetch": "^3.3.2",
    "ono": "^7.1.3",
    "ora": "^7.0.1",
    "please-upgrade-node": "^3.2.0",
    "pretty-bytes": "^6.1.1",
    "puppeteer": "^21.6.1",
    "quick-lru": "^7.0.0",
    "source-map": "^0.7.4",
    "stacktrace-parser": "^0.1.10",
    "table": "^6.8.1",
    "temp-dir": "^3.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "conventional-changelog-cli": "^2.2.2",
    "mocha": "^10.2.0",
    "navigo": "^8.11.1",
    "npm-run-all": "^4.1.5",
    "serve": "^14.2.1",
    "standard": "^17.1.0",
    "wait-for-localhost": "^4.0.1"
  },
  "keywords": [
    "memory",
    "leak",
    "detector"
  ],
  "author": "Nolan Lawson <nolan@nolanlawson.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nolanlawson/fuite.git"
  },
  "bugs": {
    "url": "https://github.com/nolanlawson/fuite/issues"
  },
  "homepage": "https://github.com/nolanlawson/fuite#readme",
  "engines": {
    "node": ">= 18.0.0"
  },
  "volta": {
    "node": "18.18.0",
    "yarn": "1.22.19"
  },
  "standard": {
    "ignore": [
      "thirdparty"
    ],
    "global": [
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "describe",
      "it",
      "AbortController"
    ]
  }
}
